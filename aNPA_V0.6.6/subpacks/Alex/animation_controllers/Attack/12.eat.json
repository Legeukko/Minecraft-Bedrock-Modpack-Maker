{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.eat": {
			"states": {
				"default": {
					"blend_transition": 0.4,
					"transitions": [
						{
							"1" : "!variable.fishing_rod && query.is_using_item && !variable.crossbow && !variable.bow && !variable.t && query.get_equipped_item_name != 'spyglass'"
						}
					]
				},
				"1": {
					"animations": [
						"eat"
					],
					"blend_transition": 0.4,
					"transitions": [
						{
							"default": "variable.fishing_rod || !query.is_using_item || variable.crossbow || variable.bow || variable.t || query.get_equipped_item_name == 'spyglass'"
						}
					]
				}
			}
		},
		"controller.animation.player.spyglass": {
			"states": {
				"default": {
					"blend_transition": 0,
					"transitions": [
						{
							"1" : "variable.is_holding_spyglass"
						}
					]
				},
				"1": {
					"animations": [
						"spyglass"
					],
					"blend_transition": 0,
					"transitions": [
						{
							"default": "!variable.is_holding_spyglass"
						}
					]
				}
			}
		}
	}
}