							"1" : "!variable.fr && query.any_animation_finished && variable.attack && variable.sword_z && !query.is_sneaking && !query.is_in_water && !query.is_sprinting && query.ground_speed<1"
						},
						{
							"default" : "variable.fr || (query.all_animations_finished && !variable.attack) || !variable.sword_z || query.is_sneaking || query.is_in_water || query.is_sprinting || query.ground_speed>1"
						}
					]
				},
				"e3": {
					"animations": [
							"default",
							"-0.5"
					],
					"blend_transition": 0.3,
					"transitions": [
						{
							"1" : "!variable.fr && query.any_animation_finished && variable.attack && variable.sword_z && !query.is_sneaking && !query.is_in_water && !query.is_sprinting && query.ground_speed<1"
						},
						{
							"default" : "variable.fr || (query.all_animations_finished && !variable.attack) || !variable.sword_z || query.is_sneaking || query.is_in_water || query.is_sprinting || query.ground_speed>1"
						}
					]
				}
			}
		}
	}
}