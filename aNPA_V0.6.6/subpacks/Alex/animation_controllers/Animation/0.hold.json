{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.player.hold.r": {
			"states": {
				"default": {
					"blend_transition": 0.2,
					"transitions": [
						{
							"1" : "variable.is_holding_right && !variable.fly_sprint_npa"
						}
					]
				},
				"1": {
					"animations": [
						"hold.r"
					],
					"blend_transition": 0.3,
					"transitions": [
						{
							"default": "!variable.is_holding_right || variable.fly_sprint_npa"
						}
					]
				}
			}
		}
	}
}