{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.player.fly.bob": {
			"states": {
				"default": {
					"blend_transition": 0.2,
					"transitions": [
						{
							"1" : "variable.fly_bob_npa"
						}
					]
				},
				"1": {
					"animations": [
						"fly.bob",
						{
						"cn.fly.bob": "!variable.t"
						}
					],
					"blend_transition": 0.2,
					"transitions": [
						{
							"default": "!variable.fly_bob_npa"
						}
					]
				}
			}
		},
		"controller.animation.player.fly.move": {
			"states": {
				"default": {
					"blend_transition": 0.2,
					"transitions": [
						{
							"1" : "variable.fly_move_npa"
						}
					]
				},
				"1": {
					"animations": [
						"fly.move",
						{
						"cn.fly.items": "!variable.bow && !variable.shield_main"
						}
					],
					"blend_transition": 0.3,
					"transitions": [
						{
							"default": "!variable.fly_move_npa"
						}
					]
				}
			}
		},
		"controller.animation.player.fly.sprint": {
			"states": {
				"default": {
					"blend_transition": 0.2,
					"transitions": [
						{
							"1" : "variable.fly_sprint_npa"
						}
					]
				},
				"1": {
					"animations": [
						"fly.sprint1",
						"fly.sprint2",
						{
						"cn.fly.items.s": "!variable.items_all"
						}
					],
					"blend_transition": 0.4,
					"transitions": [
						{
							"default": "!variable.fly_sprint_npa"
						}
					]
				}
			}
		},
		"controller.animation.player.glide.r": {
			"states": {
				"default": {
					"blend_transition": 0.2,
					"transitions": [
						{
							"1": "!variable.is_first_person && query.is_gliding && query.is_sneaking && !query.is_on_ground"
						}
					]
				},
				"1": {
					"animations": [
						"glide.r"
					],
					"blend_transition": 0.5,
					"transitions": [
						{
							"default": "variable.is_first_person || !query.is_sneaking || query.is_on_ground"
						}
					]
				}
			}
		}
	}
}