{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.player.standby": {
			"states": {
				"default": {
					"blend_transition": 0.5,
					"transitions": [
						{
							"b1" : "query.is_on_ground && !variable.attack && query.ground_speed<1 && !variable.is_first_person && !query.is_in_water && !variable.is_holding_right && !variable.is_holding_left"
						}
					]
				},
				"b1": {
					"animations": [
						"b1"
					],
					"blend_transition": 0.5,
					"transitions": [
					{
							"b2" : "!variable.attack && query.all_animations_finished && !query.ground_speed && !variable.is_first_person && !query.is_in_water && !variable.is_holding_right && !variable.is_holding_left"
						}
					]
				},
