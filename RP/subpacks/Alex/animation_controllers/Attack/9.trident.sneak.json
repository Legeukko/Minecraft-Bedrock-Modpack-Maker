{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.attack.trident.sk": {
			"states": {
				"default": {
					"blend_transition": 0.15,
					"transitions": [
						{
							"1" : "!variable.fr && variable.attack && variable.t && query.is_sneaking && !query.is_in_water && !query.is_sprinting && !query.is_using_item"
						}
					]
				},
				"1": {
					"animations": [
						"attack.trident.sk1",
						"-0.5"
					],
					"blend_transition": 0.15,
					"transitions": [
						{
							"default": "(query.all_animations_finished && !variable.attack) || variable.fr || !variable.t || !query.is_sneaking || query.is_in_water || query.is_sprinting || query.is_using_item"
						},
						{
							"2" : "query.any_animation_finished && variable.attack && !variable.fr && variable.t && query.is_sneaking && !query.is_in_water && !query.is_sprinting && !query.is_using_item"
						}
					]
				},
				"2": {
					"animations": [
						"attack.trident.sk2",
						"-0.5"
					],
					"blend_transition": 0.15,
					"transitions": [
						{
							"default": "(query.all_animations_finished && !variable.attack) || variable.fr || !variable.t || !query.is_sneaking || query.is_in_water || query.is_sprinting || query.is_using_item"
						},
						{
							"1" : "query.any_animation_finished && variable.attack && !variable.fr && variable.t && query.is_sneaking && !query.is_in_water && !query.is_sprinting && !query.is_using_item"
						}
					]
				}
			}
		}
	}
}